<!DOCTYPE html>
<html>
  <head>
    <title>gCamp</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <nav class="navbar navbar-inverse" role="navigation">
      <div class="container">
        <%= link_to "gCamp", root_path, class: "navbar-brand" %>
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to "Tasks", tasks_path %></li>
          <li><%= link_to "Users", users_path %></li>
        </ul>
      </div>
    </nav>
    <div class="container">


      <%= yield %>


      <footer>
        <hr>
        <%= link_to "About", about_path %> | <%= link_to "Terms", terms_path %> | <%= link_to "FAQ", faq_path %> | <%= link_to "Tasks", tasks_path %> | <%= link_to "Users", users_path %>
      </footer>

      <br>
      <br>

    </div>
  </body>
</html>
